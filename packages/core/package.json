
{
  "name": "@papyrus/core",
  "version": "0.1.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup index.ts --dts --format cjs,esm --out-dir dist --clean --sourcemap"
  },
  "dependencies": {
    "@papyrus/types": "0.1.0",
    "zustand": "^5.0.0"
  }
}
