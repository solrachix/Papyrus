{
  "name": "papyrus-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "dev:web": "yarn --cwd examples/web dev",
    "build:web": "yarn --cwd examples/web build",
    "build": "pnpm -r --workspace-concurrency=1 build",
    "docs:dev": "yarn --cwd examples/web build --mode docs && vitepress dev docs",
    "docs:build": "yarn --cwd examples/web build --mode docs && vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@vitejs/plugin-react": "^5.1.2",
    "tsup": "^8.3.5",
    "typescript": "^5.0.4",
    "vitepress": "^1.5.0"
  }
}
